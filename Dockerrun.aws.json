{
  "AWSEBDockerrunversion": 2,
  "containerDefinitions": [
    {
      "name": "client",
      "image": "mohangarapatidoc/multi-client",
      "hostname": "client",
      "essential": false
    },
    {
      "name": "server",
      "image": "mohangarapatidoc/multi-server",
      "hostname": "api",
      "essential": false    
    },
    {
      "name": "worker",
      "image": "mohangarapatidoc/multi-worker",
      "hostname": "worker",
      "essential": false    
    },
    {
      "name": "nginx",
      "image": "mohangarapatidoc/multi-nginx",
      "hostname": "nginx",
      "essential": true,
      "portMappings": [
        {
	  "hostPort": 80,
	  "containerPort": 80	
	}
      ],
      "links": ["client","server"]
    
    }
  ]
}
